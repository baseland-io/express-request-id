{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": [
    "@baseland-io/eslint-config-baseland"
  ],
  "root": true,
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_"
      }
    ]
  }
}
